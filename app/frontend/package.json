{
    "name": "frontend_nextjs",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 3000",
        "build": "next build",
        "build:local": "set IGNORE_SENTRY=1 && next build",
        "start": "next start -p 3000",
        "storybook": "storybook dev -p 3001",
        "build-storybook": "storybook build",
        "fixcode": "npx prettier \"**/*(*.css|*.js|*.json|*.md|*.scss)\" --write",
        "new": "node cli/bin.js new",
        "new:container": "node cli/bin.js new --container",
        "test": "jest --watch",
        "test:ci": "npx jest --ci --runInBand --reporters=default --reporters=jest-junit",
        "lint": "next lint"
    },
    "dependencies": {
        "@heroicons/react": "^2.1.1",
        "@hookform/resolvers": "^3.3.4",
        "@sentry/nextjs": "^7.76.0",
        "@types/react": "^18.2.33",
        "@typescript-eslint/eslint-plugin": "^6.20.0",
        "class-variance-authority": "^0.7.0",
        "classnames": "^2.3.2",
        "clsx": "^2.1.0",
        "cmdk": "^1.0.0",
        "i": "^0.3.7",
        "i18next": "^23.6.0",
        "lodash": "^4.17.21",
        "lucide-react": "^0.320.0",
        "next": "14.0.0",
        "next-i18next": "^14.0.0",
        "npm": "^10.4.0",
        "prop-types": "^15.8.1",
        "radix-ui": "^1.4.2",
        "react": "^18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.50.1",
        "react-i18next": "^13.3.1",
        "react-joyride": "^2.9.3",
        "react-leaflet": "^4.2.1",
        "react-leaflet-custom-control": "^1.4.0",
        "recharts": "^2.11.0",
        "tailwind": "^4.0.0",
        "tailwind-merge": "^2.2.1",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.3.3",
        "uuid": "^9.0.1",
        "zod": "^3.22.4",
        "zustand": "^4.5.2"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged --pattern 'frontend/**' && jest --onlyChanged",
            "pre-push": ""
        }
    },
    "devDependencies": {
        "@storybook/addon-a11y": "^7.5.2",
        "@storybook/addon-actions": "^7.5.2",
        "@storybook/addon-backgrounds": "^7.5.2",
        "@storybook/addon-viewport": "^7.5.2",
        "@storybook/addons": "^7.5.2",
        "@storybook/nextjs": "^7.5.2",
        "@storybook/react": "^7.5.2",
        "@swc/core": "^1.3.95",
        "@swc/jest": "^0.2.29",
        "@testing-library/jest-dom": "^6.1.4",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "autoprefixer": "^10.4.17",
        "cross-env": "^7.0.3",
        "css-loader": "^6.8.1",
        "eslint": "8.52.0",
        "eslint-config-next": "14.0.0",
        "eslint-config-prettier": "^9.0.0",
        "husky": "^8.0.3",
        "jest": "^29.6.2",
        "jest-environment-jsdom": "^29.6.2",
        "jest-junit": "^16.0.0",
        "postcss": "^8.4.33",
        "prettier": "^3.0.2",
        "pretty-quick": "^4.0.0",
        "react-test-renderer": "^18.2.0",
        "storybook": "^7.5.2",
        "style-loader": "^3.3.3",
        "tailwindcss": "^3.4.1"
    }
}
